{
  "name": "vue-inline-loading",
  "version": "1.1.0",
  "description": "Vue directive for show loading in a container",
  "main": "dist/vue-inline-loading.js",
  "scripts": {
    "dev": "webpack-dev-server --open --config webpack.dev.js",
    "build:ex": "webpack --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "release": "npm run build:ex && npm run build",
    "gh": "git subtree push --prefix examples origin gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/escX/vue-inline-loading.git"
  },
  "keywords": [
    "vue-directive",
    "loading"
  ],
  "author": "escX",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/escX/vue-inline-loading/issues"
  },
  "homepage": "https://escx.github.io/vue-inline-loading/",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "babel-loader": "^8.0.5",
    "change-case": "^3.1.0",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "project-html-template": "^1.0.0",
    "style-loader": "^0.23.1",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "peerDependencies": {
    "vue": "^2.5.0"
  }
}
